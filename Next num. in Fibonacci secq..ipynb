    "num1 = 0\n",
    "num2 = 1\n",
    "\n",
    "n = int(input(\"Enter the number of Fibonacci sequence you want to generate: \"))\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    next_num = (num1 + num2) % 10\n",
    "    num1 = num2\n",
    "    num2 = next_num\n",
    "\n",
    "print(\"The next number in the Fibonacci sequence is\", next_num)"
